import java.util.*;
class Pugazh 
{
	public static void main(String[] args) 
	{
		int[] arr = {10,20,30,40,50,60,70};
		int i=0,j=i+1;
		while (i<j)
		{
			if (i == 3 )
			{
				int temp = arr[i];
				arr[i] = arr[arr.length-1];
                arr[arr.length-1] = temp;
				
			}
			i++;
		}
		
		
		System.out.print(Arrays.toString(arr));
	}
}
