class Ball 
{
    private int radius;
	public int getRadius(){
		return radius;
	}
	public void setRadius(int radius){
		this.radius=radius;
	}
}
class BasketBall extends Ball
{
   //states
   String game;

   //const
   game(){}

   game(String game){
	this.game=game;
   }

   //behaviour


}
class TenniesBall extends Ball
{
//states
   String game;

   //const
   game(){}

   game(String game){
	this.game=game;
   }

   //behaviour
}
